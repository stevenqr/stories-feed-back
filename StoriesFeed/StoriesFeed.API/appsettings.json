{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "HackerApi": {
    "baseUrl": "https://hacker-news.firebaseio.com/v0",
    "getItemPath": "item",
    "getNewStoriesIdsPath": "newstories"
  },
  "StoriesFeedCache": {
    "key": "storiesFeedCache",
    "expirationInSeconds": 300
  }
}
